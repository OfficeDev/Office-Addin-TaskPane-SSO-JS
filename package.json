{
  "name": "office-addin-taskpane-sso",
  "version": "0.0.0",
  "private": true,
  "config": {
    "app-to-debug": "excel",
    "app-type-to-debug": "desktop"
  },
  "scripts": {
    "build": "webpack -p --mode production --https false",
    "build:dev": "webpack --mode development --https false",
    "clean": "rimraf dist",
    "configure-sso": "node ./node_modules/office-addin-sso/lib/cli.js configure manifest.xml Office-Addin-NodeJS-SSO",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "sideload": "office-addin-debugging start manifest.xml",
    "start": "concurrently \"npm run start:server\" \"npm run sideload\" ",
    "start:server": "node server.js"
  },
  "dependencies": {
    "dotenv": "^8.1.0",
    "jQuery": "^1.7.4",
    "msal": "^1.1.3",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@types/jquery": "^3.3.31",
    "@types/node": "^12.12.5",
    "@types/office-js": "^1.0.50",
    "@types/office-runtime": "^1.0.9",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "4.1.5",
    "office-addin-debugging": "^3.0.16",
    "office-addin-dev-certs": "^1.4.11",
    "office-addin-manifest": "^1.4.12",
    "concurrently": "^4.1.2",
    "cpx": "^1.5.0",
    "rimraf": "3.0.0",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.1.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.7.2",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8"
  }
